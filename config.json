{
  "name": "Photo Capture",
  "namespace": "MCRI\\PhotoCapture",
  "description": "this external module is helpful for capture image from device's camera of user rather uploading file into surveys",
  "permissions": [
    "redcap_save_record",
    "redcap_data_entry_form_top",
    "redcap_survey_page_top"
  ],
  "authors": [{
    "name": "Harneet",
    "email": "harneet.bhinder@mcri.org.au",
    "institution": "MCRI"
  }],
  "project-settings": [{
    "key": "phca_source_target_pairs",
    "name": "instrument-upload_field pair",
    "required": true,
    "type": "sub_settings",
    "repeatable": true,
    "sub_settings": [{
        "key": "phca_source_instrument",
        "name": "Name of survey instrument that should be saved to the upload field",
        "required": true,
        "type": "form-list",
        "repeatable": false
      },
      {
        "key": "phca_target_upload_field",
        "name": "Field where captured image will be saved",
        "required": true,
        "type": "field-list",
        "repeatable": false
      }
    ]
  }],
  "compatibility": {
    "redcap-version-min": "8.0.3"
  }
}
